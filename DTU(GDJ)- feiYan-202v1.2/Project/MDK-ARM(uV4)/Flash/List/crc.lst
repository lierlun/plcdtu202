N			 INT16U len    : 长度						
N** 输 　 出: CRC16码						
N********************************************************************************************************************/
Nuint16_t calCRC16(uint8_t *pSource, uint32_t uiLen)
N{
N	uint16_t i = 0;
N	uint16_t uiResult = 0;
N	uint8_t  p;
N	for (i=0; i<uiLen; i++)
N	{
N		  p = pSource[i];
N	          uiResult = (uiResult<<8) ^ ((uint16_t)CRCTable[(uiResult>>8) ^ p]);
N	}
N	return uiResult;
N}
